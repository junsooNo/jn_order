CREATE TABLE OD_PRODUCT_TB
(
PRODUCT_NO NUMBER PRIMARY KEY,
PRODUCT_NM VARCHAR2(30),
PRODUCT_PRICE NUMBER,
REG_DATE VARCHAR2(12) DEFAULT TO_CHAR(SYSDATE,'YYYYMMDDHH24MI'),
PRODUCT_IMAGE VARCHAR2(200)
);

alter table od_product_tb add(cate_no number);

CREATE SEQUENCE OD_PRODUCT_SEQ;



CREATE TABLE OD_CATEGORY_TB
(
CATE_NO NUMBER PRIMARY KEY,
CATE_NM VARCHAR2(30),
REG_DATE VARCHAR2(12) DEFAULT TO_CHAR(SYSDATE,'YYYYMMDDHH24MI'),
USE_YN VARCHAR2(1)
);


CREATE SEQUENCE OD_CATEGORY_SEQ;